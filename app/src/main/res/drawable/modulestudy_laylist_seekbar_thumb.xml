<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="#50FF9900"/>
            <size
                android:width="24dp"
                android:height="24dp"/>
        </shape>
    </item>

    <item android:bottom="6dp" android:left="6dp" android:top="6dp" android:right="6dp">
        <shape android:shape="oval">
            <solid
                android:color="#FF9600"/>
        </shape>
    </item>

</layer-list>